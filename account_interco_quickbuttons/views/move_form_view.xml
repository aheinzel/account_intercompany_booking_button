<odoo>
  <record id="view_move_form_inherit_interco_quick" model="ir.ui.view">
    <field name="name">account.move.form.interco.quick</field>
    <field name="model">account.move</field>
    <field name="inherit_id" ref="account.view_move_form"/>
    <field name="arch" type="xml">
      <xpath expr="//form/header" position="inside">
        <button name="%(action_open_interco_quick_wizard_move_bind)d"
                type="action"
                string="Quick interco booking"
                class="oe_highlight"
                context="{'default_statement_line_id': statement_line_id}"
                invisible="move_type != 'entry' or not statement_line_id"/>
      </xpath>
    </field>
  </record>
</odoo>
